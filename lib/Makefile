	cd std && $(MAKE) clean
	cd os && $(MAKE) clean
	cd net && $(MAKE) clean
	cd block && $(MAKE) clean
	cd dns && $(MAKE) clean
	cd dyntype && $(MAKE) clean
	cd http && $(MAKE) clean
	cd cow && $(MAKE) clean
#	cd debugger && $(MAKE) clean
