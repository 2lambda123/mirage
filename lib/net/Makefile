.PHONY: all clean
all:
	cd mpl && $(MAKE)
	cd api && $(MAKE)
	cd ether && $(MAKE)
	cd dhcp && $(MAKE)
	cd dns && $(MAKE)
	cd socket && $(MAKE)

clean:
	cd mpl && $(MAKE) clean
	cd api && $(MAKE) clean
	cd ether && $(MAKE) clean
	cd dhcp && $(MAKE) clean
	cd dns && $(MAKE) clean
	cd socket && $(MAKE) clean
